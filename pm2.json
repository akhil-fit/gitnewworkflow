{
    "name"             : "gitnewworkflow",
    "script"           : "app.js",
    "log_date_format"  : "YYYY-MM-DD HH:mm Z",
    "max_restarts"     : 1,
    "watch"            : true,
    "ignore_watch"      : ["node_modules","uploads",".git"],
    "exec_interpreter" : "node",
    "exec_mode"        : "cluster",
    "env": {
      "NODE_ENV": "default"
    },
    "env_test" : {
      "NODE_ENV": "test"
    },
    "env_staging": {
      "NODE_ENV": "staging"
    }
  }